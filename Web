    <!-- Toast de notificaci√≥n -->
    <div id="toast" class="toast">C√≥digo copiado al portapapeles</div>
    
    <script>
        function toggleContent(id) {
            const element = document.getElementById(id);
            
            // Cerrar todos los contenidos primero
            document.querySelectorAll('.data-content').forEach(content => {
                content.classList.remove('active');
            });
            
            // Alternar el contenido seleccionado
            element.classList.add('active');
        }
        
        // Mostrar el primer contenido por defecto
        document.addEventListener('DOMContentLoaded', function() {
            document.getElementById('datos-personales').classList.add('active');
        });
        
        // Funci√≥n para copiar el c√≥digo HTML
        function copyCode() {
            // Obtener el c√≥digo HTML completo
            const htmlCode = document.documentElement.outerHTML;
            
            // Crear un elemento textarea temporal
            const textarea = document.createElement('textarea');
            textarea.value = htmlCode;
            textarea.setAttribute('readonly', '');
            textarea.style.position = 'absolute';
            textarea.style.left = '-9999px';
            document.body.appendChild(textarea);
            
            // Seleccionar y copiar el texto
            textarea.select();
            document.execCommand('copy');
            
            // Eliminar el textarea temporal
            document.body.removeChild(textarea);
            
            // Mostrar notificaci√≥n
            showToast('C√≥digo HTML copiado al portapapeles');
        }
        
        // Funci√≥n para exportar como PDF
        function exportToPDF() {
            // Ocultar temporalmente los botones de acci√≥n para la impresi√≥n
            const actionButtons = document.querySelector('.action-buttons');
            actionButtons.style.display = 'none';
            
            // Usar la funci√≥n de impresi√≥n del navegador
            window.print();
            
            // Restaurar los botones despu√©s de un breve retraso
            setTimeout(() => {
                actionButtons.style.display = 'flex';
            }, 1000);
        }
        
        // Funci√≥n para mostrar notificaci√≥n toast
        function showToast(message) {
            const toast = document.getElementById('toast');
            toast.textContent = message;
            toast.classList.add('show');
            
            // Ocultar el toast despu√©s de 3 segundos
            setTimeout(() => {
                toast.classList.remove('show');
            }, 3000);
        }
    </script>
    
    <style>
        @media print {
            body {
                background: white;
                padding: 0;
                margin: 0;
            }
            
            .action-buttons, .toast {
                display: none !important;
            }
            
            .max-w-4xl {
                max-width: none;
                margin: 0;
                box-shadow: none;
            }
            
            /* Asegurar que todo el contenido sea visible */
            .data-content {
                display: block !important;
                opacity: 1 !important;
                page-break-inside: avoid;
                margin-bottom: 20px;
            }
        }
    </style>
